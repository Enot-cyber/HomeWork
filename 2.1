def matches(a, b):
    S = 0
    for i in range(0, len(a)):
        m = a[i]
        for j in range(0, len(b)):
            if m == b[j]:
                S += 1
    return S
if __name__ == "__main__":
    print(matches("Aa", "aAAccDSAD"))
    print(matches("Bb", "aAAccDSAD"))
    print(matches("Dd", "aAAccDSAD"))
